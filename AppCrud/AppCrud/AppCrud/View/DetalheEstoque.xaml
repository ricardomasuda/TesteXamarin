<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppCrud.View.DetalheEstoque"
               BackgroundColor="#F5F6FA"
             >
    <ContentPage.Content>
        <Frame
            Margin="10"
            >
            <StackLayout>
                <StackLayout>
                    <Label Text="Nome:" TextColor="Black" FontSize="12"/>
                    <Entry BackgroundColor="Gray"/>
                </StackLayout>
                <StackLayout>
                    <Label Text="Qunatidade:" TextColor="Black" FontSize="12"/>
                    <Entry BackgroundColor="Gray" Keyboard="Numeric"/>
                </StackLayout>
                <StackLayout>
                    <Label Text="Data:" TextColor="Black" FontSize="12"/>
                    <Entry BackgroundColor="Gray" />
                </StackLayout>
                <Button
                    Text="Cadastrar"
                    />
            </StackLayout>
        </Frame>
        
    </ContentPage.Content>
</ContentPage>